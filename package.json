{
  "name": "empty_project_boilerplate",
  "version": "0.0.1",
  "author": "YOU",
  "license": "MIT",
  "scripts": {
    "dev": "tsx src/index.ts",
    "dev:watch": "tsx watch src/index.ts",
    "start": "node dist/index.js",
    "build": "tsc --noEmit && tsup src",
    "test:commit": "vitest run --exclude \"**/*.e2e.spec.ts\"",
    "test:unit": "vitest run --exclude \"**/*.(e2e|it).spec.ts\"",
    "test:it": "vitest run \".it.spec.ts\"",
    "test:e2e": "vitest run \".e2e.spec.ts\"",
    "test:watch": "vitest watch",
    "test:cov": "vitest run --coverage --exclude \"**/*.(e2e|it).spec.ts\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "prepare": "husky"
  },
  "dependencies": {
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "mongoose": "^8.3.2",
    "morgan": "^1.10.0",
    "tsup": "^8.0.2",
    "winston": "^3.13.0"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/express": "^4.17.21",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.12.7",
    "@types/sinon": "^17.0.3",
    "@types/winston": "^2.4.4",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitest/coverage-v8": "^1.5.0",
    "axios": "^1.6.8",
    "eslint": "^8.42.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "husky": "^9.0.11",
    "mongodb-memory-server": "^9.2.0",
    "prettier": "^3.0.0",
    "sinon": "^17.0.1",
    "tsx": "^4.7.2",
    "typescript": "^5.1.3",
    "vitest": "^1.5.0"
  }
}
